<Window x:Class="GenerateCerts.Windows.LoadTxt"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="LoadTxt" Height="364" Width="360">
    <Grid>
        <Button Content="Ok" HorizontalAlignment="Right" Margin="0,0,10,10" VerticalAlignment="Bottom" Width="75" Click="Button_Click"/>
        <Button Content="Cancel" HorizontalAlignment="Right" Margin="0,0,90,10" VerticalAlignment="Bottom" Width="75" Click="Button_Click_1"/>
        <TextBox x:Name="domains" Margin="10,10,10,35" TextWrapping="Wrap" AcceptsReturn="True"/>
        <CheckBox x:Name="chk_reset" Content="Reset list" HorizontalAlignment="Right" Margin="0,0,275,10" VerticalAlignment="Bottom"/>
        
    </Grid>
</Window>
